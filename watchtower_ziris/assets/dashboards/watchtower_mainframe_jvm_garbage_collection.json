        "layout_type": "ordered",
        "widgets": [
          {
            "id": 6124499827412004,
            "definition": {
              "type": "note",
              "content": "Top 10 applications with highest GC duration.\n",
              "background_color": "green",
              "font_size": "14",
              "text_align": "left",
              "vertical_align": "center",
              "show_tick": true,
              "tick_pos": "50%",
