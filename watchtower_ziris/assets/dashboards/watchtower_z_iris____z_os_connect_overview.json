          {
            "id": 7870388388312153,
            "definition": {
              "type": "note",
              "content": "Triggered monitors relevant to APIs hosted on mainframe. Monitors must  have the tag \"mainframe-apis\"configured be included. \n\n",
              "background_color": "green",
              "font_size": "14",
              "text_align": "left",
              "vertical_align": "center",
              "show_tick": true,
              "tick_pos": "50%",
