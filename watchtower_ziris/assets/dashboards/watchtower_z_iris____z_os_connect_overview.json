            },
            "layout": {
              "x": 8,
              "y": 0,
              "width": 4,
              "height": 1
            }
          },
          {
            "id": 1412979145035409,
            "definition": {
              "title": "Request Count",
              "title_size": "16",
              "title_align": "left",
              "time": {},
              "type": "query_value",
              "requests": [
                {
                  "formulas": [
                    {
                      "formula": "query1"
                    }
                  ],
                  "queries": [
                    {
                      "data_source": "metrics",
                      "name": "query1",
                      "query": "sum:zos.connect.http.request.methods{$host, $sysplex.name, $server.name, $service}.rollup(sum, 60)",
                      "aggregator": "sum"
                    }
                  ],
                  "response_format": "scalar"
                }
              ],
              "autoscale": true,
              "precision": 0
            },
            "layout": {
              "x": 0,
              "y": 1,
              "width": 4,
              "height": 2
            }
          },
          {
            "id": 1710411261509064,
            "definition": {
              "title": "Error Rate",
              "title_size": "16",
              "title_align": "left",
              "time": {},
              "type": "query_value",
              "requests": [
                {
                  "formulas": [
                    {
                      "number_format": {
                        "unit": {
                          "type": "canonical_unit",
                          "unit_name": "percent"
                        }
                      },
                      "formula": "((default_zero(query1) + default_zero(query2)) / default_zero(query3)) * 100"
                    }
                  ],
                  "queries": [
                    {
                      "data_source": "metrics",
                      "name": "query1",
                      "query": "sum:zos.connect.http.response.codes{http.response.status_code:4*, $host, $sysplex.name, $server.name, $service}.rollup(sum, 60)",
                      "aggregator": "sum"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query2",
                      "query": "sum:zos.connect.http.response.codes{http.response.status_code:5*, $host, $sysplex.name, $server.name, $service}.rollup(sum, 60)",
                      "aggregator": "sum"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query3",
                      "query": "sum:zos.connect.http.response.codes{$host, $server.name, $sysplex.name, $service}.rollup(sum, 60)",
                      "aggregator": "sum"
                    }
                  ],
                  "response_format": "scalar",
                  "conditional_formats": [
                    {
                      "comparator": ">",
                      "value": 0,
                      "palette": "white_on_red"
                    },
                    {
                      "comparator": "=",
                      "value": 0,
                      "palette": "white_on_green"
                    }
                  ]
                }
              ],
              "autoscale": false,
              "precision": 2
            },
            "layout": {
              "x": 4,
              "y": 1,
              "width": 4,
              "height": 2
            }
          },
          {
            "id": 3419234784546059,
