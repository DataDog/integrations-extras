            "id": 7183467356638811,
            "definition": {
              "title": "Top 5 Request Count by API",
              "title_size": "16",
              "title_align": "left",
              "type": "query_table",
              "requests": [
                {
                  "queries": [
                    {
                      "data_source": "metrics",
                      "name": "query1",
                      "query": "sum:zos.connect.successful_requests{$host, $sysplex.name, $server.name, $service} by {service}.rollup(sum, 60)",
                      "aggregator": "sum"
                    },
                    {
                      "data_source": "metrics",
                      "name": "query2",
                      "query": "sum:zos.connect.timed_out_requests{$host, $sysplex.name, $server.name, $service} by {service}.rollup(sum, 60)",
                      "aggregator": "sum"
                    }
                  ],
                  "response_format": "scalar",
                  "sort": {
                    "count": 5,
                    "order_by": [
                      {
                        "type": "formula",
                        "index": 0,
                        "order": "desc"
                      }
                    ]
                  },
                  "formulas": [
                    {
                      "alias": "Processed",
                      "number_format": {
                        "unit": {
                          "type": "canonical_unit",
                          "unit_name": "request"
                        }
                      },
                      "formula": "default_zero(query1)"
                    },
                    {
                      "alias": "Timed Out",
                      "number_format": {
                        "unit": {
                          "type": "canonical_unit",
                          "unit_name": "request"
                        }
                      },
                      "formula": "default_zero(query2)"
