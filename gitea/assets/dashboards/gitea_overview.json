            "definition": {
              "title": "Check Status",
              "title_size": "16",
              "title_align": "left",
              "type": "check_status",
              "check": "gitea.openmetrics.health",
              "grouping": "check",
              "group": "endpoint:http://localhost:3000/metrics,host:COMP-C02G70X8ML87",
