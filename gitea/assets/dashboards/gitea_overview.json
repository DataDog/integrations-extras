          {
            "id": 7197768260374900,
            "definition": {
              "title": "Memory",
              "title_size": "16",
              "title_align": "left",
              "type": "query_value",
              "requests": [
                {
                  "formulas": [
                    {
                      "formula": "query1"
                    }
                  ],
                  "response_format": "scalar",
                  "queries": [
                    {
                      "query": "avg:gitea.process.virtual_memory.bytes{endpoint:$gitea_url.value}",
                      "data_source": "metrics",
                      "name": "query1",
                      "aggregator": "avg"
                    }
                  ]
                }
              ],
              "autoscale": true,
              "precision": 2,
              "timeseries_background": {
                "type": "area",
                "yaxis": {
                  "include_zero": true
                }
              }
            },
            "layout": {
              "x": 4,
              "y": 0,
              "width": 4,
              "height": 2
            }
          },
          {
            "id": 8649701631096988,
            "definition": {
              "title": "% of open files",
              "title_size": "16",
              "title_align": "left",
              "type": "query_value",
              "requests": [
                {
                  "formulas": [
                    {
                      "formula": "(query1 / query2) * 100"
                    }
                  ],
                  "conditional_formats": [
                    {
                      "comparator": "<",
                      "palette": "white_on_green",
                      "value": 50
                    },
                    {
                      "comparator": "<",
                      "palette": "white_on_yellow",
                      "value": 75
                    },
                    {
                      "comparator": ">=",
                      "palette": "white_on_red",
                      "value": 75
                    }
                  ],
                  "response_format": "scalar",
                  "queries": [
                    {
                      "query": "avg:gitea.process.open_fds{endpoint:$gitea_url.value}",
                      "data_source": "metrics",
                      "name": "query1",
                      "aggregator": "avg"
                    },
                    {
                      "query": "avg:gitea.process.max_fds{endpoint:$gitea_url.value}",
                      "data_source": "metrics",
                      "name": "query2",
                      "aggregator": "avg"
                    }
                  ]
                }
              ],
              "autoscale": false,
              "custom_unit": "%",
              "precision": 1
            },
            "layout": {
              "x": 8,
              "y": 0,
              "width": 4,
              "height": 2
            }
          },
          {
            "id": 4203639177313164,
            "definition": {
