          {
            "id": 5241756259399710,
            "definition": {
              "title": "Average Data Disk Throughput",
              "title_size": "16",
              "title_align": "left",
              "show_legend": true,
              "legend_layout": "auto",
              "legend_columns": [
                "avg",
                "min",
                "max",
                "value",
                "sum"
              ],
              "type": "timeseries",
              "requests": [
                {
                  "formulas": [
                    {
                      "alias": "$host - Read",
                      "number_format": {
                        "unit": {
                          "type": "canonical_unit",
                          "unit_name": "byte"
                        }
                      },
                      "formula": "derivative(query1) * (-1) * 1000 * 512"
                    },
                    {
                      "alias": "$host - Write",
                      "formula": "derivative(query2) * 1000 * 512"
                    }
                  ],
                  "queries": [
                    {
                      "name": "query1",
                      "data_source": "metrics",
                      "query": "avg:ocient.local_storage_service.data_units_read{$Cluster} by {host}"
                    },
                    {
                      "name": "query2",
                      "data_source": "metrics",
                      "query": "avg:ocient.local_storage_service.data_units_write{$Cluster} by {host}"
                    }
