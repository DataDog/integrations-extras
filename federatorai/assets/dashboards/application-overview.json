            "id": 0,
            "definition": {
                "type": "timeseries",
                "requests": [
                    {
                        "q": "sum:kubernetes.cpu.usage.total{$kube_cluster,$kube_namespace,$kube_deployment,$kube_stateful_set}/1000000",
