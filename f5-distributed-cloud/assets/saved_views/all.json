{
    "timerange": {
        "interval_ms": 86400000
    },
    "query": "source:*",
    "name": "F5XC | All logs",
    "visible_facets": [
        "@browser_type",
        "@connection_state",
        "@domain",
        "@dst_port",
        "@hostname",
        "@latitude",
        "@longitude",
        "@method",
        "@namespace",
        "@rsp_code",
        "@rsp_code_class",
        "@rsp_code_details",
        "@rtt_downstream_seconds",
        "@rtt_upstream_seconds",
        "@severity",
        "@src",
        "@src_ip"
    ],
    "page": "stream",
    "type": "logs",
    "options": {
        "show_date_column": true,
        "message_display": "inline",
        "show_message_column": true,
        "show_timeline": true,
        "columns": [
            "@hostname",
            "@namespace",
            "content"
        ]
    }
}