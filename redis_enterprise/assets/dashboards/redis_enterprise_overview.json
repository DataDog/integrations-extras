    {
      "id": 325090501093728,
      "definition": {
        "title": "About Redis Enterprise",
        "banner_img": "https://raw.githubusercontent.com/DataDog/integrations-extras/58e0302ab52d7e1425cdcc5eb14dd16b9898db9e/redis_enterprise/assets/logos/logo.svg",
