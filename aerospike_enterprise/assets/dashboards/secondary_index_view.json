              "requests": [
                {
                  "response_format": "scalar",
                  "queries": [
                    {
                      "data_source": "metrics",
                      "name": "query1",
                      "query": "sum:aerospike.server.sindex.entries_per_bval{$aerospike_cluster, $ns, $sindex}",
                      "aggregator": "last"
                    }
                  ],
                  "conditional_formats": [
                    {
                      "comparator": ">=",
                      "value": 0,
                      "palette": "white_on_green"
                    }
                  ],
                  "formulas": [
                    {
                      "formula": "query1"
                    }
                  ]
                }
              ],
              "autoscale": true,
              "precision": 2,
              "timeseries_background": {
                "type": "area",
                "yaxis": {
                  "include_zero": true
