{
  "title": "k6",
  "description": "This dashboard allows visualizing some of the performance testing metrics collected by [k6](https://k6.io/). \n\nFor further information about exporting metrics from k6 OSS, read the [Datadog's k6 integration docs](https://k6.io/docs/getting-started/results-output/datadog).\n\nFor more information about the integration with the k6 Cloud, visit [Cloud APM docs](https://k6.io/docs/cloud/integrations/cloud-apm).\n\nClone this template dashboard to make changes and add other graph widgets.",
  "author_name": "k6",
  "widgets": [
    {
      "id": 0,
      "definition": {
        "type": "timeseries",
        "requests": [
          {
            "q": "max:k6.vus{$test_run_id}",
            "display_type": "line",
            "style": {
              "palette": "dog_classic",
              "line_type": "solid",
              "line_width": "normal"
            }
          }
        ],
        "custom_links": [],
        "yaxis": {
          "label": "",
          "scale": "linear",
          "min": "auto",
          "max": "auto",
          "include_zero": true
        },
        "title": "Virtual users",
        "title_size": "16",
        "title_align": "left",
        "show_legend": false
      },
      "layout": {
        "x": 14,
        "y": 0,
        "width": 47,
        "height": 15
      }
    },
    {
      "id": 1,
      "definition": {
        "type": "timeseries",
        "requests": [
          {
            "q": "avg:k6.http_reqs{$test_run_id}.as_rate()",
            "display_type": "line",
            "style": {
              "palette": "dog_classic",
              "line_type": "solid",
              "line_width": "normal"
            }
          }
        ],
        "custom_links": [],
        "yaxis": {
          "label": "",
          "scale": "linear",
          "min": "auto",
          "max": "auto",
          "include_zero": true
        },
        "title": "Request per second",
        "title_size": "16",
        "title_align": "left",
        "show_legend": false
      },
      "layout": {
        "x": 14,
        "y": 17,
        "width": 47,
        "height": 15
      }
    },
    {
      "id": 2,
      "definition": {
        "type": "timeseries",
        "requests": [
          {
            "q": "cumsum(avg:k6.data_sent{$test_run_id})",
            "metadata": [
              {
                "expression": "cumsum(avg:k6.data_sent{$test_run_id})",
                "alias_name": "data_sent"
              }
            ],
            "display_type": "line",
            "style": {
              "palette": "dog_classic",
              "line_type": "solid",
              "line_width": "normal"
            }
          },
          {
            "q": "cumsum(avg:k6.data_received{$test_run_id})",
            "metadata": [
              {
                "expression": "cumsum(avg:k6.data_received{$test_run_id})",
                "alias_name": "data_received"
              }
            ],
            "display_type": "line",
            "style": {
              "palette": "dog_classic",
              "line_type": "solid",
              "line_width": "normal"
            }
          }
        ],
        "custom_links": [],
        "yaxis": {
          "label": "",
          "scale": "linear",
          "min": "auto",
          "max": "auto",
          "include_zero": true
        },
        "events": [
          {
            "q": "tag:test_run_id:22",
            "tags_execution": "and"
          }
        ],
        "title": "Data sent/received",
        "title_size": "16",
        "title_align": "left",
        "show_legend": false
      },
      "layout": {
        "x": 14,
        "y": 34,
        "width": 47,
        "height": 15
      }
    },
