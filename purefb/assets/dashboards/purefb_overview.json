      {
         "id":6967691037749466,
         "definition":{
            "title":"Arrays",
            "title_size":"16",
            "title_align":"left",
            "type":"query_table",
            "requests":[
               {
                  "response_format":"scalar",
                  "queries":[
                     {
                        "query":"min:purefb.hardware.health{$ArrayName,$Env} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query1",
                        "aggregator":"last"
                     },
                     {
                        "query":"sum:purefb.alerts.open{$ArrayName,severity:*,$Env} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query4",
                        "aggregator":"last"
                     },
                     {
                        "query":"avg:purefb.array.space_data_reduction_ratio{$ArrayName,$Env,type:array} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query5",
                        "aggregator":"last"
                     },
                     {
                        "query":"sum:purefb.array.space_bytes{$ArrayName,$Env,type:array,space:capacity} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query7",
                        "aggregator":"last"
                     },
                     {
                        "query":"min:purefb.array.space_bytes{$ArrayName,$Env,type:array,space:total_physical} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query6",
                        "aggregator":"last"
                     }
                  ],
                  "sort":{
                     "count":500,
                     "order_by":[
                        {
                           "type":"formula",
                           "index":0,
                           "order":"desc"
                        }
                     ]
                  },
                  "formulas":[
                     {
                        "alias":"Array Health",
                        "conditional_formats":[
                           {
                              "palette":"white_on_red",
                              "comparator":"<",
                              "value":1
                           },
                           {
                              "palette":"white_on_green",
                              "comparator":">=",
                              "value":1
                           }
                        ],
                        "cell_display_mode":"number",
                        "formula":"query1"
                     },
                     {
                        "alias":"Total Alerts",
                        "conditional_formats":[
                           {
                              "palette":"white_on_yellow",
                              "value":0,
                              "comparator":">"
                           }
                        ],
                        "cell_display_mode":"number",
                        "formula":"query4"
                     },
                     {
                        "alias":"Data Reduction Ratio",
                        "conditional_formats":[
                           {
                              "palette":"white_on_red",
                              "value":1,
                              "comparator":"<"
                           },
                           {
                              "palette":"white_on_green",
                              "value":1,
                              "comparator":">="
                           }
                        ],
                        "cell_display_mode":"number",
                        "formula":"query5"
                     },
                     {
                        "alias":"Array Capacity",
                        "cell_display_mode":"number",
                        "formula":"query7"
                     },
                     {
                        "alias":"Array Free Space",
                        "cell_display_mode":"number",
                        "conditional_formats":[
                           {
                              "palette":"white_on_red",
                              "comparator":"<",
                              "value":1000000000000
                           },
                           {
                              "palette":"white_on_green",
                              "comparator":">=",
                              "value":1000000000000
                           }
                        ],
                        "formula":"query7 - query6"
                     },
                     {
                        "alias":"Utilization %",
                        "conditional_formats":[
                           {
                              "palette":"white_on_green",
                              "comparator":"<",
                              "value":80
                           },
                           {
                              "palette":"white_on_yellow",
                              "comparator":">=",
                              "value":80
                           },
                           {
                              "palette":"white_on_red",
                              "comparator":">=",
                              "value":90
                           }
                        ],
                        "cell_display_mode":"number",
                        "formula":"query6 / query7 * 100"
                     }
                  ]
               }
            ],
            "has_search_bar":"auto"
         },
         "layout":{
            "x":4,
            "y":0,
            "width":8,
            "height":2
         }
      },
      {
         "id":2917915264466940,
         "definition":{
            "type":"note",
            "content":"## Overview\n\n* This dashboard provides performance and capacity details for Pure Storage FlashBlades.\n* Group arrays by customer defined environment, whole fleets, or view arrays individually.",
            "background_color":"white",
            "font_size":"14",
            "text_align":"left",
            "vertical_align":"top",
            "show_tick":false,
            "tick_pos":"50%",
            "tick_edge":"left",
            "has_padding":true
         },
         "layout":{
            "x":0,
            "y":2,
            "width":4,
            "height":2
         }
      },
      {
         "id":2624012081925984,
         "definition":{
            "title":"Alerts",
            "title_size":"16",
            "title_align":"left",
            "type":"query_table",
            "requests":[
               {
                  "response_format":"scalar",
                  "queries":[
                     {
                        "query":"sum:purefb.alerts.open{$ArrayName,severity:info,$Env} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query1",
                        "aggregator":"last"
                     },
                     {
                        "query":"sum:purefb.alerts.open{$ArrayName,severity:warning,$Env} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query2",
                        "aggregator":"last"
                     },
                     {
                        "query":"sum:purefb.alerts.open{$ArrayName,severity:critical,$Env} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query3",
                        "aggregator":"last"
                     },
                     {
                        "query":"sum:purefb.alerts.open{$ArrayName,severity:*,$Env} by {fb_array_name}",
                        "data_source":"metrics",
                        "name":"query4",
                        "aggregator":"last"
                     }
                  ],
                  "sort":{
                     "count":500,
                     "order_by":[
                        {
                           "type":"formula",
                           "index":2,
                           "order":"desc"
                        }
                     ]
                  },
                  "formulas":[
                     {
                        "alias":"Info",
                        "conditional_formats":[
                           {
                              "palette":"black_on_light_yellow",
                              "value":1,
                              "comparator":">="
                           }
                        ],
                        "cell_display_mode":"number",
                        "formula":"query1"
                     },
                     {
                        "alias":"Warning",
                        "conditional_formats":[
                           {
                              "palette":"white_on_yellow",
                              "value":1,
                              "comparator":">="
                           }
                        ],
                        "cell_display_mode":"number",
                        "formula":"query2"
                     },
                     {
                        "alias":"Critical",
                        "conditional_formats":[
                           {
                              "palette":"white_on_red",
                              "value":1,
                              "comparator":">="
                           }
                        ],
                        "cell_display_mode":"number",
                        "formula":"query3"
                     },
                     {
                        "alias":"Total Alerts",
                        "cell_display_mode":"number",
                        "formula":"query4"
                     }
                  ]
               }
            ],
            "has_search_bar":"auto"
         },
         "layout":{
            "x":4,
            "y":2,
            "width":8,
            "height":2
         }
