        "layout": {
          "x": 100,
          "y": 21,
          "width": 47,
          "height": 15
        }
      },
      {
        "id": 3,
        "definition": {
          "type": "check_status",
          "title": "Speedtest Run Status",
          "title_size": "16",
          "title_align": "left",
          "check": "speedtest.run",
          "grouping": "check",
          "group": "host:pi64",
          "group_by": [],
          "tags": [
            "*"
          ]
        },
        "layout": {
          "x": 20,
          "y": 1,
          "width": 16,
          "height": 12
        }
      },
      {
        "id": 4,
        "definition": {
          "type": "image",
          "url": "/static/images/logos/speedtest_large.svg",
          "sizing": "zoom"
        },
        "layout": {
          "x": 3,
          "y": 1,
          "width": 16,
          "height": 12
        }
      },
      {
        "id": 5,
        "definition": {
          "type": "note",
          "content": "Packet Loss",
          "background_color": "gray",
          "font_size": "18",
          "text_align": "center",
          "show_tick": true,
          "tick_pos": "50%",
          "tick_edge": "bottom"
        },
        "layout": {
          "x": 100,
          "y": 15,
          "width": 47,
          "height": 5
        }
      },
