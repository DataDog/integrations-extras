      {
        "id": 12,
        "definition": {
          "type": "query_table",
          "requests": [
            {
              "q": "avg:speedtest.download.bandwidth{*} by {isp}",
              "aggregator": "avg",
              "limit": 50,
              "order": "desc",
              "alias": "Avg Download"
            },
            {
              "q": "avg:speedtest.upload.bandwidth{*} by {isp}",
              "aggregator": "avg",
              "alias": "Avg Upload"
            },
            {
              "q": "sum:speedtest.download.bytes{*} by {isp}",
              "aggregator": "sum",
              "alias": "Total Download"
            },
            {
              "q": "sum:speedtest.upload.bytes{*} by {isp}",
              "aggregator": "sum",
              "alias": "Total Upload"
            }
