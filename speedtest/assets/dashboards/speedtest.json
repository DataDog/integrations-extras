{
    "title": "Speedtest",
    "description": "## Speedtest Dashboard\n\nThis dashboard provides quick views of the key Speedtest metrics from your integration check.\n\n- [Speedtest Integration Overview](https://github.com/DataDog/integrations-extras/blob/master/speedtest/README.md)\n- [Speedtest CLI](https://www.speedtest.net/apps/cli)",
    "widgets": [
      {
        "id": 0,
        "definition": {
          "type": "timeseries",
          "requests": [
            {
              "q": "avg:speedtest.ping.jitter{*,*,*} by {isp}",
              "display_type": "line",
              "style": {
                "palette": "dog_classic",
                "line_type": "solid",
                "line_width": "normal"
              }
            }
          ],
          "custom_links": [],
          "yaxis": {
            "label": "",
            "scale": "linear",
            "min": "auto",
            "max": "auto",
            "include_zero": true
          },
          "title": "Ping Jitter (ms)",
          "title_size": "16",
          "title_align": "left",
          "show_legend": false,
          "legend_size": "0"
        },
        "layout": {
          "x": 51,
          "y": 21,
          "width": 47,
          "height": 15
        }
      },
      {
        "id": 1,
        "definition": {
          "type": "timeseries",
          "requests": [
            {
              "q": "avg:speedtest.ping.latency{*,*,*} by {isp}",
              "display_type": "line",
              "style": {
                "palette": "dog_classic",
                "line_type": "solid",
                "line_width": "normal"
              }
            }
          ],
          "custom_links": [],
          "yaxis": {
            "label": "",
            "scale": "linear",
            "min": "auto",
            "max": "auto",
            "include_zero": true
          },
          "title": "Ping Latency (ms)",
          "title_size": "16",
          "title_align": "left",
          "show_legend": false,
          "legend_size": "0"
        },
        "layout": {
          "x": 3,
          "y": 21,
          "width": 47,
          "height": 15
        }
      },
      {
        "id": 2,
        "definition": {
          "type": "timeseries",
          "requests": [
            {
              "q": "avg:speedtest.packet_loss{*,*,*} by {isp}",
              "display_type": "line",
              "style": {
                "palette": "dog_classic",
                "line_type": "solid",
                "line_width": "normal"
              }
            }
          ],
          "custom_links": [],
          "yaxis": {
            "label": "",
            "scale": "linear",
            "min": "auto",
            "max": "auto",
            "include_zero": true
