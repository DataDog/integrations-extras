{
  "version": 2,
  "title": "High CU Computation",
  "tags": [
    "integration:zilliz-cloud-zilliz-cloud"
  ],
  "created_at": "2024-11-08",
  "last_updated_at": "2024-11-08",
  "description": "CU Computation measures the utilized computational power relative to the total computational capacity of the CU. This monitor tracks the high CU computation rate to stay ahead of potential issues. ",
  "definition": {
    "id": 157660939,
    "name": "[Zilliz Cloud] CU Computation is higher than usual",
    "type": "query alert",
    "query": "avg(last_10m):avg:zilliz.cluster.cu.computation.current{*} by {org_id,project_id,cluster_id} * 100 > 90",
    "message": "Current value:  {{value}} %",
    "tags": [
      "integration:zilliz"
    ],
    "options": {
      "thresholds": {
        "critical": 90,
        "warning": 70
      },
      "notify_audit": false,
      "on_missing_data": "default",
      "include_tags": true,
      "new_group_delay": 60,
      "silenced": {}
    },
    "priority": null,
    "restricted_roles": null
  }
}