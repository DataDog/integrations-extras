{
  "version": 2,
  "title": "High CU Capacity",
  "tags": [
    "integration:zilliz-cloud-zilliz-cloud"
  ],
  "created_at": "2024-11-08",
  "last_updated_at": "2024-11-08",
  "description": "CU Capacity measures the used capacity relative to the total capacity of the CU. This monitor tracks high CU Capacity to stay ahead of potential issues.",
  "definition": {
    "id": 157661814,
    "name": "[Zilliz Cloud] CU Capacity is higher than usual",
    "type": "query alert",
    "query": "avg(last_10m):avg:zilliz.cluster.cu.capacity.current{*} by {org_id,project_id,cluster_id} * 100 > 90",
    "message": "Current value:  {{value}} %",
    "tags": [
      "integration:zilliz"
    ],
    "options": {
      "thresholds": {
        "critical": 90,
        "warning": 70
      },
      "notify_audit": false,
      "on_missing_data": "default",
      "include_tags": true,
      "new_group_delay": 60,
      "silenced": {}
    },
    "priority": null,
    "restricted_roles": null
  }
}