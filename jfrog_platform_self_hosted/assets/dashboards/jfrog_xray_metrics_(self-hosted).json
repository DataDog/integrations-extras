{
    "author_name": "Datadog",
    "description": "This dashboard provides an overview of the system performance of JFrog Xray.  Included also, are some high-level statistics on the number and type of artifacts and components that are scanned by Xray.\n\nExample configuration and setup are also available at [Github](https://github.com/jfrog/log-analytics-datadog) ",
    "layout_type": "ordered",
    "template_variables": [
        {
            "available_values": [],
            "default": "*",
            "name": "PackageType",
            "prefix": "package_type"
        }
    ],
    "title": "JFrog Xray Metrics (Self-hosted)",
    "widgets": [
        {
            "definition": {
                "sizing": "cover",
                "type": "image",
                "url": "https://raw.githubusercontent.com/DataDog/integrations-extras/master/jfrog_platform_self_hosted/images/xray.png"
            },
            "id": 3579529804067270,
            "layout": {
                "height": 2,
                "width": 2,
                "x": 0,
                "y": 0
            }
        },
        {
            "definition": {
                "legend_columns": [
                    "avg",
                    "min",
                    "max",
                    "value",
                    "sum"
                ],
                "legend_layout": "auto",
                "markers": [],
                "requests": [
                    {
                        "display_type": "line",
                        "formulas": [
                            {
                                "alias": "Disk Free Size",
                                "formula": "query1"
                            }
                        ],
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "avg:jfrog.xray.app_disk_free_bytes{*}"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "dog_classic"
                        }
                    },
                    {
                        "display_type": "line",
                        "formulas": [
                            {
                                "alias": "Disk Used Size",
                                "formula": "query1"
                            }
                        ],
                        "on_right_yaxis": false,
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "avg:jfrog.xray.app_disk_used_bytes{*}"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "cool"
                        }
                    },
                    {
                        "display_type": "line",
                        "formulas": [
                            {
                                "alias": "Sys Memory Free Size",
                                "formula": "query1"
                            }
                        ],
                        "on_right_yaxis": false,
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "avg:jfrog.xray.sys_memory_free_bytes{*}"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "warm"
                        }
                    },
                    {
                        "display_type": "line",
                        "formulas": [
                            {
                                "alias": "Sys Memory Used Size",
                                "formula": "query1"
                            }
                        ],
                        "on_right_yaxis": false,
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "avg:jfrog.xray.sys_memory_used_bytes{*}"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "purple"
                        }
                    },
                    {
                        "display_type": "line",
                        "formulas": [
                            {
                                "alias": "Sys CPU Ratio",
                                "formula": "query1"
                            }
                        ],
                        "on_right_yaxis": true,
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "avg:jfrog.xray.sys_cpu_ratio{*}"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "orange"
                        }
                    }
                ],
                "right_yaxis": {
                    "include_zero": true,
                    "label": "",
                    "max": "auto",
                    "min": "auto",
                    "scale": "linear"
                },
                "show_legend": true,
                "title": "Xray System Metrics",
                "title_align": "left",
                "title_size": "16",
                "type": "timeseries",
                "yaxis": {
                    "include_zero": true,
                    "label": "",
                    "max": "auto",
                    "min": "auto",
                    "scale": "linear"
                }
            },
            "id": 5598058711878380,
            "layout": {
                "height": 4,
                "width": 10,
                "x": 2,
                "y": 0
            }
        },
        {
            "definition": {
                "background_color": "gray",
                "content": "Tracks system performance metrics associated with JFrog Xray\n",
                "font_size": "14",
                "has_padding": true,
                "show_tick": true,
                "text_align": "center",
                "tick_edge": "right",
                "tick_pos": "50%",
                "type": "note",
                "vertical_align": "center"
            },
            "id": 7593791732506644,
            "layout": {
                "height": 2,
                "width": 2,
                "x": 0,
                "y": 2
            }
        },
        {
            "definition": {
                "legend_columns": [
                    "avg",
                    "min",
                    "max",
                    "value",
                    "sum"
                ],
                "legend_layout": "auto",
                "markers": [],
                "requests": [
                    {
                        "display_type": "line",
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "sum:jfrog.xray.jfxr_data_artifacts_total{$PackageType} by {package_type}.as_count()"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "dog_classic"
                        }
                    }
                ],
                "show_legend": true,
                "title": "Xray Scanned Artifacts Data Metrics",
                "title_align": "left",
                "title_size": "16",
                "type": "timeseries",
                "yaxis": {
                    "include_zero": true,
                    "label": "",
                    "max": "auto",
                    "min": "auto",
                    "scale": "linear"
                }
            },
            "id": 4710270725792214,
            "layout": {
                "height": 4,
                "width": 6,
                "x": 0,
                "y": 4
            }
        },
        {
            "definition": {
                "legend_columns": [
                    "avg",
                    "min",
                    "max",
                    "value",
                    "sum"
                ],
                "legend_layout": "auto",
                "markers": [],
                "requests": [
                    {
                        "display_type": "line",
                        "on_right_yaxis": false,
                        "queries": [
                            {
                                "data_source": "metrics",
                                "name": "query1",
                                "query": "sum:jfrog.xray.jfxr_data_components_total{$PackageType} by {package_type}.as_count()"
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "dog_classic"
                        }
                    }
                ],
                "show_legend": true,
                "title": "Xray Scanned Components Data Metrics",
                "title_align": "left",
                "title_size": "16",
                "type": "timeseries",
                "yaxis": {
                    "include_zero": true,
                    "label": "",
                    "max": "auto",
                    "min": "auto",
                    "scale": "linear"
                }
            },
            "id": 7617532773060648,
            "layout": {
                "height": 4,
                "width": 6,
                "x": 6,
                "y": 4
            }
        }
    ]
}
